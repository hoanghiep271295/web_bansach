<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://cloudbookstore.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>cloudbookstore.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>cloudbookstore</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$cloudbookstore</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="MyDBContext" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:bookstore273.database.windows.net,1433;Initial Catalog=bookstore;Persist Security Info=False;User ID=cuongnguyen;Password=ABCabc123;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" Name="Server=tcp:bookstore273.database.windows.net,1433;Initial Catalog=bookstore;Persist Security Info=False;User ID=cuongnguyen;Password=ABCabc123;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Web_HoangHiep.Models.MyDBContext, Web_HoangHiep" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)MyDBContext-Web.config Connection String">
      <ParameterValue>Server=tcp:bookstore273.database.windows.net,1433;Initial Catalog=bookstore;Persist Security Info=False;User ID=cuongnguyen;Password=ABCabc123;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>